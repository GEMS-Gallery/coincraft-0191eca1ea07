type Transaction = 
 record {
   amount: float64;
   description: text;
   id: nat;
   timestamp: int;
 };
type Holding = 
 record {
   id: nat;
   name: text;
   value: float64;
 };
service : {
  addHolding: (text, float64) -> (nat);
  addTransaction: (float64, text) -> (nat);
  getBalance: () -> (float64) query;
  getHoldings: () -> (vec Holding) query;
  getTransactions: () -> (vec Transaction) query;
}
